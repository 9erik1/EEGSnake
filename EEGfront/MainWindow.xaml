<Window x:Class="EEGfront.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="720" Width="1280">
    <Grid KeyDown="Wave_KeyDown">
        <WindowsFormsHost Name="GLWindow"  Panel.ZIndex="50"  ></WindowsFormsHost>

        <ComboBox IsSynchronizedWithCurrentItem="True" ItemsSource="{Binding MenuItem}" SelectedValue="{Binding CurrentOption}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120">
            
        </ComboBox>





    </Grid>
</Window>
